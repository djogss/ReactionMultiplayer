<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.aity.game.myreactionmultiplayer">

    <uses-sdk android:minSdkVersion="9" android:targetSdkVersion="16"/>

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.aity.game.myreactionmultiplayer.activity.Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.aity.game.myreactionmultiplayer.activity.ReactionGame" android:noHistory="true"/>
        <activity android:name="com.aity.game.myreactionmultiplayer.activity.FinishGame" android:noHistory="true"/>
    </application>


</manifest>
